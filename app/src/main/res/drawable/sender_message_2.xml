<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item>
    <shape android:shape="rectangle">
        <gradient
            android:type="radial"
            android:gradientRadius="58dp"
            android:centerX="50%"
            android:centerY="0%"
